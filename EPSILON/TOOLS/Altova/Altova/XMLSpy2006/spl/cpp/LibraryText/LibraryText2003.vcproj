	<Configurations>
		<Configuration
			Name="Unicode Debug|Win32"
			OutputDirectory=".\\UnicodeDebug"
			IntermediateDirectory=".\\UnicodeDebug"
[if $libtype = 1]
			ConfigurationType="4"
[else]
			ConfigurationType="2"
[endif]
			UseOfMFC="[=$MFCCodeVS7]"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="1">
			<Tool
				Name="VCCLCompilerTool"
				AdditionalOptions="/Zm200 "
				Optimization="0"
				AdditionalIncludeDirectories="..\\Altova,..\\AltovaText"
[if $libtype = 1]
				PreprocessorDefinitions="_LIB;_DEBUG;WIN32;_WINDOWS;UNICODE"
	[if $mfc]
				RuntimeLibrary="3"
	[else]
				RuntimeLibrary="1"
	[endif]
[else]
				PreprocessorDefinitions="[=$module]_EXPORTS;_USRDLL;_DEBUG;WIN32;_WINDOWS;UNICODE"
				RuntimeLibrary="3"
[endif]
				BasicRuntimeChecks="3"
				RuntimeTypeInfo="TRUE"
				UsePrecompiledHeader="3"
				PrecompiledHeaderThrough="stdafx.h"
				PrecompiledHeaderFile=".\\UnicodeDebug/[=$module].pch"
				AssemblerListingLocation=".\\UnicodeDebug/"
				ObjectFile=".\\UnicodeDebug/"
				ProgramDataBaseFileName=".\\UnicodeDebug/"
				BrowseInformation="1"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="4"
				CompileAs="0"/>
[if $libtype = 1]
			<Tool
				Name="VCLibrarianTool"
				OutputFile=".\\UnicodeDebug\\[=$module].lib"
				SuppressStartupBanner="TRUE"/>
[else]
			<Tool
				Name="VCLinkerTool"
				OutputFile=".\\UnicodeDebug/[=$module].dll"
				LinkIncremental="1"
				SuppressStartupBanner="TRUE"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile=".\\UnicodeDebug/[=$module].pdb"
				ImportLibrary=".\\UnicodeDebug/[=$module].lib"
				TargetMachine="1"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="_DEBUG"
				MkTypLibCompatible="TRUE"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName=".\\UnicodeDebug/[=$module].tlb"
				HeaderFileName=""/>
			<Tool
				Name="VCPostBuildEventTool"
				CommandLine="@mkdir ..\\[=$AppProjectName]\\UnicodeDebug &gt;nul
@copy .\\UnicodeDebug\\[=$module].dll ..\\[=$AppProjectName]\\UnicodeDebug\\ &gt;nul
"/>
[endif]
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="_DEBUG"
